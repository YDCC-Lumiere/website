{{ define "info" }}
{{ template "header" }}
<div class="container mt-3 fs-3">
    <div class="row">
        <div class="col-10 offset-md-1" id="doc-content">
            <h3 class="display-3">About us</h3>
            {{ range . }}
            <div class="name-badge border border-2 border-warning rounded-1">
                <div class="image">
                    <img src="{{ .Img }}" class="rounded-1"></img>
                </div>
                <p class="name fs-5">{{ .Name }}</p>
                <p class="desc fs-6">{{ .Desc }}</p>
            </div>
            {{ end }}
        </div>
    </div>
</div>
<script src="static/crossorigin-form.js"></script>
{{ template "footer" }}
{{ end }}
